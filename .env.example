# Workato OEM API
WORKATO_API_TOKEN=your_api_token_here
WORKATO_BASE_URL=https://www.workato.com/api
WORKATO_DATACENTER=us

# Optional: Restrict to specific customer IDs (comma-separated)
# WORKATO_CUSTOMERS=12345,E67890

# For testing: run on a single OEM account only (overrides WORKATO_CUSTOMERS when set)
# WORKATO_TEST_ACCOUNT_ID=12345

# Anthropic (for documentation generation)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Optional: Use different models (default: claude-3-5-haiku, claude-3-5-sonnet for quality)
# ANTHROPIC_DOC_MODEL=claude-3-5-haiku-20241022
# ANTHROPIC_QUALITY_MODEL=claude-3-5-sonnet-20241022

# Scheduler (cron expression, default: every 6 hours)
CRON_SCHEDULE=0 */6 * * *

# Output directory for FileSystem publisher
OUTPUT_DIR=output

# Log level: trace, debug, info, warn, error
LOG_LEVEL=info
